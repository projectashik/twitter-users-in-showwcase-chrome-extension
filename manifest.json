{
  "name": "Twitter users in Showwcase",
  "version": "0.1",
  "description": "Twitter users in Showwcase",
  "author": "Ashik Chapagian",
  "manifest_version": 3,
  "icons": {
    "16": "icons-16x16.png",
    "32": "icons-38x38.png",
    "48": "icons-48x48.png",
    "128": "icons-128x128.png"
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"]
    }
  ]
}
